#!/usr/bin/env python
"""
DACETS - facets of datasets

Usage:
  dct [new] [name]

Options:
  -h --help     Show help.
  -v --version  Show version.
"""

from docopt import docopt
from dacets import (__title__, __version__)

if __name__ == '__main__':
    args = docopt(__doc__, version='%s v%s' % (__title__, __version__))
    print(args)
